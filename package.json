{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm:watch-css\" \"npm:watch-dev\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "watch-dev": "remix dev",
    "watch-css": "postcss styles --base styles --dir app/styles -w",
    "build-css": "postcss styles --base styles --dir app/styles --env production"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@remix-run/react": "^1.0.4",
    "@remix-run/serve": "^1.0.4",
    "concurrently": "^6.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.4",
    "remix-auth": "^2.4.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
